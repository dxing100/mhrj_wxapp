/**index.wxss**/
.my-swiper image{
  width: 100%;
}

.wrap{
	display: inline-block;
	margin-top: .8rem;
}
.wrap-item{
	width: 33%;
	height: 6rem;
	border-bottom: 0px solid #ddd;
	background-color: #fff;
	text-align: center;
}
.icon_back{
    width: 25%;
    height:100%;
    float: left;
}
/*图片*/
.image{
    width:100%;
    height:100%;
    ime-mode: aspectFill;
}
.icon_style{
	width:50px;
	height:50px;
	display:table;
	margin-left:auto;
	margin-right:auto;
	margin-top:15px;
	padding-bottom:2px;
}
.icon_txt{
	display:table;
	margin-left:auto;
	margin-right:auto;
	font-size:12px;
	margin-top:10px;
}